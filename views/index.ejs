<style>
    *{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    text-decoration: none;
}
nav{
    width: 100%;
    height: 80px;
}
nav ul li img{
    margin-top: 0px;
}
nav ul{
    float:left;
    margin-left: 2.5%;
}
nav ul li{
    display: inline;
    line-height: 80px;
    font-size: 14px;
    padding: 10px;   
    font-family: sans-serif;
}
nav ul li a{
    color: #979A9B;
}
nav ul li a:hover{
    color: black;
    font-weight: bold;
}
#check{
    display: none;
}
#checkbtn{
    float: right;
    padding-right: 20px;
    padding-left: 20px;
    line-height: 80px;
    font-size: 18px;
}
@media (max-width:705px) {
    #title{
        font-size: 30px;
        display: flex;
        justify-content: center;
        align-items: center;
    }
    nav ul{
        width: 100%;
        height: 100vh;
        background-color: red;
        position: fixed;
        left: -100%;
    }
    nav ul li{
        display: block;
        text-align: center;
    }

}
@media (min-width:705px) {
    #checkbtn{
        display: none;
    }

}
#check:checked~ul{
    left: 0%;
    transition: all 0.7s;
} 
